<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
	<div class="col-sm-4">
		<%=image_tag @admin_profile.image.url,:class=>"profile_img"%>
	</div>
	<div class="col-sm-6">
		<div class="profile_detail">
			<p>Email:
			<%=@admin_profile.email%>
			</p>
		</div>
		<div class="profile_detail">
			<p>Address:
			<%=@admin_profile.address%>
			</p>
		</div>
		<div class="profile_detail">
			<p>Phone Number:
			<%=@admin_profile.phone_number%>
			</p>
		</div>
	</div>
</div>

