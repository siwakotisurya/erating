<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
	<h1 class="page-header"> Welcome To Dashboard</h1>
	<div class="error_form">
	<%if @student.errors.any?%>
	<%@student.errors.full_messages.each do |s|%>
	<%=s%><br>
	<%end%>
	<%end%>
	</div>
	<div class="form-group">
	<%=form_for @student, url: {:action=>"create"}, html:{:role=>"form"} do |f|%>
		<label for="email">Full Name:</label>
		<div class="form-group">
		<%=f.text_field :full_name, :class=>"form_control", :id=>"email"%>
		</div>
		<label for="pwd">Email Address:</label>
		<div class="form-group">
		<%=f.text_field :email_address, :class=>"form_control", :id=>"pwd"%>
		</div>
		<label for="pwd">Phone Number:</label>
		<div class="form-group">
		<%=f.text_field :phone_number, :class=>"form_control"%>
		</div>
		<div class="form-group">
		<%=f.submit("Submit", :class=>"btn btn-primary")%>
		</div>
	<%end%>
</div>
</div>
