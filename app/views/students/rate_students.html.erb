<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
	<h1 class="page-header"> Student List</h1>
	<h2> sad</h2>
	<table class="table table-hover">
		<thead>
     <tr>
       <th>Full Name</th>
        <th>Email</th>
        <th>Phone No</th>
        <th>Edit</th>
      </tr>
    </thead>
		<tbody>
			<%@students.each do |s|%>
      <tr>
        <td><%=s.full_name%></td>
        <td><%=s.email_address%></td>
        <td><%=s.phone_number%></td>
        <td><%= button_to "Grade", new_grading_path(s.id),:class =>"btn btn-primary"%></td>
      </tr>
   		<%end%>
    </tbody>
  </table>
</div>
